{
  "name": "language-tplpre",
  "description": "BMC tpl and tplpre language support in Atom and VS Code",
  "version": "3.4.0",
  "publisher": "trianglesis",
  "categories": ["Programming Languages", "Snippets"],
  "author": {
    "name": "trianglesis"
  },
  "engines": {
    "atom": "*",
    "node": "*",
    "vscode": "*"
  },
  "contributes": {
    "languages": [
      {
        "id": "tplpre",
        "aliases": ["TPL", "tplpre", "tpl"],
        "extensions": [".tpl", ".tplpre"]
      },
      {
        "id": "model",
        "aliases": ["model"],
        "extensions": [".model"]
      }
    ],
    "grammars": [
      {
        "language": "tplpre",
        "scopeName": "source.tplpre",
        "path": "./grammars/tplpre.json"
      },
      {
        "language": "tplpre",
        "scopeName": "source.grammar_check",
        "path": "./grammars/test_grammar.json"
      },
      {
        "language": "model",
        "scopeName": "source.model.tplpre",
        "path": "./grammars/tplpre.json"
      }
    ],
    "snippets": [
      {
        "language": "tplpre",
        "scopeName": ".source.tplpre",
        "path": "./snippets/language-tplpre.json"
      }
    ]
  },
  "homepage": "https://github.com/trianglesis/language-tplpre",
  "repository": {
    "type": "git",
    "url": "https://github.com/trianglesis/language-tplpre.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trianglesis/language-tplpre/issues"
  },
  "devDependencies": {
    "coffeelint": "^1.10.1"
  },
  "keywords": [
    "language-tplpre",
    "tplpre",
    "tpl",
    "syntax",
    "template",
    "grammar",
    "discovery",
    "BMC"
  ]
}
